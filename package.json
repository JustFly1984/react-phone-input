    "eslint-plugin-react-functional-set-state": "1.2.1",
    "eslint-plugin-react-hooks": "1.6.0",
    "eslint-plugin-react-perf": "3.1.0",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.5.0",
    "husky": "2.3.0",
    "jest": "24.8.0",
